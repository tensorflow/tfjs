{
  "name": "@tensorflow/tfjs-vis",
  "version": "1.4.3",
  "description": "Utilities for in browser visualization with TensorFlow.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tensorflow/tfjs.git",
    "directory": "tfjs-vis"
  },
  "license": "Apache-2.0",
  "private": false,
  "main": "dist/index.js",
  "jsdelivr": "dist/tfjs-vis.umd.min.js",
  "unpkg": "dist/tfjs-vis.umd.min.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && NODE_ENV=production webpack",
    "lint": "tslint -p . -t verbose",
    "coverage": "KARMA_COVERAGE=1 karma start --browsers='Chrome' --singleRun",
    "test": "yarn && yarn build && karma start",
    "test-dev": "karma start",
    "run-flaky": "node ../scripts/run_flaky.js",
    "run-browserstack": "karma start --singleRun --reporters='dots,karma-typescript,BrowserStack' --hostname='bs-local.com'",
    "test-ci": "./scripts/test-ci.sh",
    "build-npm": "./scripts/build-npm.sh",
    "link-local": "yalc link",
    "publish-local": "rimraf dist/ && yarn build && yalc push"
  },
  "dependencies": {
    "d3-format": "~3.1.0",
    "d3-selection": "~3.0.0",
    "glamor": "~2.20.40",
    "preact": "~8.2.9",
    "vega": "5.20.0",
    "vega-embed": "6.17.0",
    "vega-lite": "4.13.1"
  },
  "devDependencies": {
    "@tensorflow/tfjs-backend-webgl": "4.3.0",
    "@tensorflow/tfjs-core": "4.3.0",
    "@tensorflow/tfjs-layers": "4.3.0",
    "@types/d3-format": "~3.0.1",
    "@types/d3-selection": "~3.0.4",
    "@types/jasmine": "~4.3.1",
    "@types/json-stable-stringify": "~1.0.32",
    "babel-core": "~6.26.3",
    "babel-plugin-external-helpers": "~6.22.0",
    "babel-preset-env": "~1.7.0",
    "clang-format": "~1.8.0",
    "jasmine": "~4.5.0",
    "jasmine-core": "~4.5.0",
    "karma": "^6.4.1",
    "karma-browserstack-launcher": "~1.6.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-firefox-launcher": "~2.1.2",
    "karma-jasmine": "~5.1.0",
    "karma-safari-launcher": "~1.0.0",
    "karma-typescript": "~5.5.3",
    "karma-typescript-es6-transform": "~5.5.3",
    "npm-run-all": "~4.1.5",
    "preact-render-spy": "~1.3.0",
    "rimraf": "~4.1.2",
    "tslint": "~6.1.3",
    "tslint-no-circular-imports": "~0.7.0",
    "typescript": "4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "yalc": "~1.0.0-pre.50"
  },
  "peerDependencies": {
    "@tensorflow/tfjs-core": ">= 1.0.0"
  },
  "alias": {
    "react": "preact",
    "react-dom": "preact"
  },
  "resolutions": {
    "node-fetch": "2.6.7",
    "minimist": "1.2.6"
  }
}
