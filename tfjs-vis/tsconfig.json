{
  "extends": "../tsconfig",
  "include": [
    "src/"
  ],
  "exclude": [
    "node_modules/"
  ],
  "compilerOptions": {
    "module": "commonjs",
    "inlineSources": true,
    "target": "es2015",
    "lib": [
      "es2015",
      "dom"
    ],
    "noUnusedLocals": false,
    "strictNullChecks": false,
    "pretty": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "jsxFactory": "h",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "./dist"
  }
}
