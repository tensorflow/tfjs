{
  "name": "@tensorflow/tfjs-backend-webgpu",
  "version": "0.0.1-alpha.4",
  "main": "dist/tf-backend-webgpu.node.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "dist/index.js",
  "module": "dist/index.js",
  "unpkg": "dist/tf-backend-webgpu.min.js",
  "jsdelivr": "dist/tf-backend-webgpu.min.js",
  "miniprogram": "dist/miniprogram",
  "scripts": {
    "benchmark": "yarn && yarn build-deps && karma start --grep=benchmark --browsers=chrome_webgpu",
    "build-ci": "tsc && yarn bundle-ci",
    "build": "rimraf dist/ && tsc && yarn bundle",
    "bundle": "rollup -c",
    "bundle-ci": "rollup -c --ci",
    "build-link-package-core": "cd ../link-package-core && yarn build",
    "build-deps": "yarn build-link-package-core",
    "build-deps-ci": "yarn build-link-package-core",
    "build-npm": "./scripts/build-npm.sh",
    "link-local": "yalc link",
    "publish-local": "rimraf dist/ && yarn build && rollup -c && yalc push",
    "publish-npm": "./scripts/publish-npm.sh",
    "lint": "tslint -p . -t verbose",
    "test": "yarn && yarn build-deps && karma start --browsers=chrome_webgpu --excludeTest=src/benchmark_*_test.ts",
    "test-dev": "karma start --browsers=chrome_webgpu --excludeTest=src/benchmark_*_test.ts",
    "test-ci": "./scripts/test-ci.sh",
    "unlink-local": "yalc remove"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/polyfill": "^7.8.7",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-typescript": "^3.0.0",
    "@tensorflow/tfjs-core": "link:../link-package-core/node_modules/@tensorflow/tfjs-core",
    "@types/jasmine": "~3.0.0",
    "clang-format": "~1.5.0",
    "jasmine": "~3.1.0",
    "jasmine-core": "~3.1.0",
    "karma": "~6.3.1",
    "karma-browserstack-launcher": "~1.6.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-commonjs": "^1.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-typescript": "~5.5.1",
    "karma-typescript-es6-transform": "^5.0.2",
    "npm-run-all": "~4.1.3",
    "rimraf": "~2.6.2",
    "rollup": "~2.3.2",
    "rollup-plugin-terser": "~7.0.2",
    "rollup-plugin-visualizer": "~3.3.2",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.3",
    "tslint-no-circular-imports": "~0.7.0",
    "typescript": "3.5.3",
    "yalc": "~1.0.0-pre.50"
  },
  "dependencies": {
    "@tensorflow/tfjs-backend-cpu": "link:../link-package-core/node_modules/@tensorflow/tfjs-backend-cpu",
    "@webgpu/glslang": "0.0.12",
    "@webgpu/types": "0.1.4"
  },
  "peerDependencies": {
    "@tensorflow/tfjs-core": "link:../link-package-core/node_modules/@tensorflow/tfjs-core"
  }
}
