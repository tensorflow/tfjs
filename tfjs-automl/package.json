    "ts-lint": "^4.5.1",
    "ts-node": "^8.8.2",
    "tslint": "~5.11.0",
    "tslint-no-circular-imports": "^0.5.0",
    "typescript": "3.5.3",
    "yalc": "~1.0.0-pre.21"
