    "@tensorflow/tfjs-backend-webgl": "^3.0.0-rc.1",
    "@types/jasmine": "~2.8.6",
    "clang-format": "~1.2.4",
    "jasmine-core": "~3.6.0",
    "karma": "~4.2.0",
    "karma-browserstack-launcher": "~1.6.0",
    "karma-chrome-launcher": "~2.2.0",
