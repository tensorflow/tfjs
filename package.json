{
  "name": "tfjs-converter",
  "version": "0.1.0",
  "description": "Tensorflow model converter for javascript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/tfjsconverter.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tensorflow/web.git"
  },
  "license": "Apache-2.0",
  "peerDependencies": {
    "deeplearn": "^0.5.0"
  },
  "devDependencies": {
    "@types/jasmine": "~2.8.6",
    "@types/seedrandom": "~2.4.27",
    "browserify": "~16.1.0",
    "clang-format": "~1.2.2",
    "cross-spawn": "~6.0.4",
    "http-server": "~0.11.1",
    "jasmine-core": "~3.1.0",
    "karma": "~2.0.0",
    "karma-browserstack-launcher": "~1.3.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-firefox-launcher": "~1.0.1",
    "karma-jasmine": "~1.1.1",
    "karma-typescript": "~3.0.12",
    "opn": "~5.1.0",
    "shelljs": "~0.7.8",
    "tsify": "~3.0.1",
    "tslint": "~5.8.0",
    "tslint-no-circular-imports": "~0.2.0",
    "typedoc": "~0.8.0",
    "typescript": "^2.7.2",
    "uglify-js": "~3.0.28",
    "watchify": "~3.9.0",
    "handlebars": "^4.0.11",
    "copyfiles": "^1.2.0",
    "deeplearn": "^0.5.0"
  },
  "scripts": {
    "prep": "yarn && mkdirp dist",
    "build": "tsc && copyfiles -f src/data/compiled_api.* dist/data && copyfiles -f src/operations/op_list/*.json dist/operations/op_list && copyfiles -f src/operations/typings.d.ts dist/operations",
    "test": "karma start",
    "test-travis": "karma start --browsers='bs_firefox_mac,bs_chrome_mac' --singleRun",
    "lint": "tslint -p . -t verbose",
    "make-version": "sh -c ./scripts/make-version"
  },
  "browserify-shim": {
    "deeplearn": "global:dl"
  },
  "dependencies": {
    "@types/long": "^3.0.32",
    "protobufjs": "^6.8.0"
  }
}
