    "tslint": "~5.9.1",
    "typescript": "2.7.2",
    "yalc": "~1.0.0-pre.21"
  },
  "scripts": {
    "build": "tsc",
    "build-npm": "./scripts/build-npm.sh",
    "link-local": "yalc link",
    "publish-local": "yarn build-npm && yalc push",
    "lint": "tslint -p . -t verbose",
