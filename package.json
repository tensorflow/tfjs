{
  "name": "@tensorflow/tfjs-node",
  "version": "0.1.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "gypfile": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/tensorflow/tfjs-node.git"
  },
  "scripts": {
    "prep": "cd node_modules/@tensorflow/tfjs-core && yarn && yarn build",
    "build": "tsc",
    "clean": "rm -rf deps/tensorflow/*",
    "format": "clang-format -i -style=Google binding/*.cc binding/*.h",
    "build-npm": "./scripts/build-npm.sh",
    "lint": "tslint -p . -t verbose",
    "test": "ts-node src/run_tests.ts",
    "publish-local": "yarn prep && yarn build && yalc push"
  },
  "devDependencies": {
    "@tensorflow/tfjs-core": "~0.9.1",
    "@types/bindings": "~1.3.0",
    "@types/jasmine": "~2.8.6",
    "@types/node": "~9.6.2",
    "clang-format": "~1.2.2",
    "jasmine": "~3.1.0",
    "rimraf": "~2.6.2",
    "ts-node": "~5.0.0",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "yalc": "~1.0.0-pre.21"
  },
  "dependencies": {
    "bindings": "~1.3.0"
  },
  "peerDependencies": {
    "@tensorflow/tfjs-core": "~0.9.1"
  }
}
